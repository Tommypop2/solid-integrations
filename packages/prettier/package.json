{
  "name": "@solid-integrations/prettier",
  "version": "0.0.100",
  "description": "An integration with prettier to format code",
  "author": "Thomas Beer",
  "contributors": [],
  "license": "MIT",
  "homepage": "https://github.com/tommypop2/solid-integrations/tree/main/packages/prettier#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tommypop2/solid-integrations.git"
  },
  "bugs": {
    "url": "https://github.com/tommypop2/solid-integrations/issues"
  },
  "primitive": {
    "name": "prettier",
    "stage": 0,
    "list": [
      "createFormatted"
    ],
    "category": "Utilities"
  },
  "keywords": [
    "solid",
    "primitives"
  ],
  "private": false,
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "browser": {},
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": "./dist/index.cjs"
  },
  "typesVersions": {},
  "scripts": {
    "dev": "jiti ../../scripts/dev.ts",
    "build": "jiti ../../scripts/build.ts",
    "test": "vitest -c ../../configs/vitest.config.ts",
    "test:ssr": "pnpm run test --mode ssr"
  },
  "peerDependencies": {
    "solid-js": "^1.6.12"
  },
  "dependencies": {
    "@types/prettier": "^2.7.2",
    "prettier": "^2.8.7"
  }
}
